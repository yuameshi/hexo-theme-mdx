<div class="friendLinks">
	<%- css(['css/friends.css']) %> 
	<% for (let link in site.data.friends) { %>
		<div class="mdui-card friendLinkItem">
			<div class="mdui-card-media">
				<img src="<%-site.data.friends[link].avatar%>" alt="<%-link%>" class="friendLinkImg">
				<div class="mdui-card-media">
					<div class="mdui-card-primary">
						<div class="mdui-card-primary-title">
							<%-link%>
						</div>
						<div class="mdui-card-primary-subtitle">
							<%-site.data.friends[link].description%>
						</div>
					</div>
					<div class="mdui-card-actions">
						<a class="mdui-btn mdui-ripple mdui-ripple-white" href="<%-site.data.friends[link].link%>">
							访问
						</a>
					</div>
				</div>
			</div>
		</div>
	<% } %>
</div>